#include <stdio.h>

int main() {
    char ch;
    printf("enter a character");
    scanf("%s",&ch);
    if (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='I'||ch=='E'||ch=='O'||ch=='U')
    printf("its a vowel");
    else
    printf("its a consonent");
    return 0;
    
}
    
    
    
