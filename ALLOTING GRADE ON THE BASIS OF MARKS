#include <stdio.h>

int main() {
    // Write C code here
    int marks;
    printf("enter your marks");
    scanf("%d",&marks);
    if(marks>=90&&marks<100)
    printf("A");
    else if (marks>=75)
    printf("B");
    else if (marks>=50)
    printf("C");
    else 
    printf("FAIL");
    


    return 0;
}
